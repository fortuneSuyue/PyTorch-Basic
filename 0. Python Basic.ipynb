    "class Animal(object):\n",
    "    def __init__(self, name): # initialization\n",
    "        self.name = name\n",
    "    def move(self):\n",
    "        print(\"I am going...\")\n",
    "    def attack(self):\n",
    "        print(\"Attack!\")\n",
    "        \n",
    "class Tiger(Animal):\n",
    "    def bark(self):\n",
    "        print(\"Urrr...\")\n",
    "        \n",
    "class Cat(Animal):\n",
    "    def bark(self):\n",
    "        print(\"Mew~\")\n",
    "\n",
    "tiger = Tiger(\"tiger\")\n",
    "tiger.move()\n",
    "tiger.bark()\n",
    "print(\"\\n\")\n",
    "\n",
    "cat = Cat(\"cat\")\n",
    "cat.attack()\n",
    "cat.bark()"
   ]
